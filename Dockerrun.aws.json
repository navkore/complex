{
    "AWSEBDockerrunVersion": 2,
    "ContainerDefinitions": [
        {
            "name": "client",
            "image": "mmzaki/multi-client",
            "hostName": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "mmzaki/multi-server",
            "hostName": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "mmzaki/multi-worker",
            "hostName": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "mmzaki/multi-nginx",
            "hostName": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "server"
            ],
            "memory": 128
        }
    ]
}
